package com.example.batuhan.app;

import com.example.batuhan.proje.Account;
//ctrl + shift + o

public class AccountApp {

	public static void main(String[] args) {

		Account acc;
		acc = new Account("TR1", 100_000);
		
		acc.withdraw(27_500);
		acc.withdraw(7_500);

		System.out.println("Balance: " + acc.getBalance());
		acc.deposit(32_500);
		System.out.println("Balance: " + acc.getBalance());
	}

}
